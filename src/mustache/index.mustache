<!DOCTYPE html>
<html>
  <head>
    <title>SpaceChat</title>
    <link rel="stylesheet/less" type="text/css" href="/stylesheets/main.less" />
    {{{baseJavascripts}}}
    {{#clientTemplate}}loremIpsum{{/clientTemplate}}
    <script type="text/javascript" src="/javascripts/main.js"></script>
  </head>
  <body>
    <div class="grid">
      <div class="column wide" id="leftPane">
        <div id="chat">
        </div>
        <div id="compose">
          <div id="textContainer">
            <textarea rows="0" cols="0" id="text"></textarea>
          </div>
        </div>
      </div>

      <div class="column narrow">
        <div id="participants">
        </div>
      </div>
    </div>
  </body>
</html>
